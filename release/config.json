{
    "warnOnUnexpectedLayer": "true",
    "wv-options-fetch": [
        {
            "from": "http://geoworldview.ssec.wisc.edu/onearth/wmts/epsg4326/wmts.cgi?request=GetCapabilities",
            "to": "gibs-geographic.xml"
        },
        {
            "from": "https://gibs.earthdata.nasa.gov/wmts/epsg4326/best/wmts.cgi?request=GetCapabilities",
            "to": "gibs-geographic_nasa.xml"
        },
        {
            "from": "https://sips.ssec.wisc.edu/onearth/wmts/epsg4326/wmts.cgi?request=GetCapabilities",
            "to": "sips-geographic.xml"
        }
    ],
    "wv-options-wmts": [
        {
            "source": "GIBS:geographic",
            "from": "gibs-geographic.xml",
            "to": "gibs-geographic.json",
            "projection": "geographic",
            "maxResolution": 0.5625
        },
        {
            "source": "GIBS:geographic_nasa",
            "from": "gibs-geographic_nasa.xml",
            "to": "gibs-geographic_nasa.json",
            "projection": "geographic",
            "maxResolution": 0.5625
        },
        {
            "source": "SIPS:geographic",
            "from": "sips-geographic.xml",
            "to": "sips-geographic.json",
            "projection": "geographic",
            "maxResolution": 0.5625
        }

    ]
}
