{
    "warnOnUnexpectedLayer": "true",
    "wv-options-fetch": [
        {
            "from": "http://mayon.ssec.wisc.edu/onearth/wmts/epsg4326/wmts.cgi?request=GetCapabilities",
            "to": "gibs-geographic.xml"
        }
    ],
    "wv-options-wmts": [
        {
            "source": "GIBS:geographic",
            "from": "gibs-geographic.xml",
            "to": "gibs-geographic.json",
            "projection": "geographic",
            "maxResolution": 0.5625
        }
    ]
}
